<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="edu.uw.tcss450.labose.signinandregistration.ui.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_contacts"
        android:name="edu.uw.tcss450.labose.signinandregistration.ui.HomeFragment"
        android:label="Contacts"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_chat"
        android:name="edu.uw.tcss450.labose.signinandregistration.ui.chat.ChatFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="edu.uw.tcss450.labose.signinandregistration.theme.FragmentSetting"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" />
        <action
            android:id="@+id/action_navigation_settings_to_theme"
            app:destination="@id/navigation_settings_themes" />

    <fragment
        android:id="@+id/navigation_settings_themes"
        android:name="edu.uw.tcss450.labose.signinandregistration.theme.ThemeFragment"
        android:label="fragment_theme"
        tools:layout="@layout/fragment_theme" />
</navigation>